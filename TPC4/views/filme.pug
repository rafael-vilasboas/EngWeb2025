extends layout

block content
  .w3-card-4
    header.w3-container.w3-blue
      h1= title
    .w3-container
      h2 #{filme.title}
      h3 #{filme.year}
      h3 Atores:
      ul.w3-ul.w3-hoverable
        each ator in filme.cast
          li
            a(href="/ator/" + ator onmouseover="this.style='color: white'" onmouseout="this.style='color: black'")= ator
